{
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-unused-vars": "warn",
      "quotes": ["error", "single", { "avoidEscape": true }],
      "no-var": ["error"],
      "no-console": "off"
    }
  }
  