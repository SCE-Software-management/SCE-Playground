{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-unused-vars": ["warn"],
    "no-console": "off",
    "indent": ["error", 2],
    "eqeqeq": ["error", "always"],
    "curly": "error"
  }
}
